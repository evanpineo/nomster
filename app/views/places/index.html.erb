<h1><% @places.each do |place| %></h1>
    <div class="shadow-box col-10 offset-1">
        <h1><%= place.name %></h1><br>
        <i><%= place.address %></i><br><br>
        <p><%= place.description %></p><br>
    </div>
    
<% end %>

<br>
<div class="center">
    <%= will_paginate @places %>
</div>
